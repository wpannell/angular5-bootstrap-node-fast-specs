{
  "license": "larry steck and jason calalang",
  "name": "garage-dev-bootcamp",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0",
    "yarn": "1.5.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run spec"
    }
  },
  "scripts": {
    "ng": "ng",
    "ci": "concurrently \"npm run lint:ci\" \"npm run server:lint:ci\" \" npm run spec:ci\" \" npm run server:spec:ci\" \" npm run spec:coverage:ci\" \" npm run server:coverage:ci\"",
    "start": "node_modules/.bin/ng serve --environment=prod",
    "start:local": "concurrently --kill-others \"npm run server\" \"ng serve --environment=local\" ",
    "start-qa": "ng serve --environment=qa --port 4300",
    "server": "json-server server/api/db.json --port 8080 --watch",
    "build": "node --stack_size=4048 node_modules/.bin/ng build --prod --environment=prod",
    "lint": "ng lint",
    "lint:ci": "npm run lint -s",
    "test": "npm run spec",
    "prepush": "npm run lint && npm run spec",
    "server:lint": "esw -f simple-detail --clear server",
    "server:lint:ci": "npm run server:lint -s",
    "server:lint:watch": "npm run --silent server:lint -- -w",
    "server:start": "webpack --colors --progress --config webpack.server.config.js",
    "server:start:reload": "npm run server:start & browser-sync start --proxy 'localhost:3001' --files 'dist-server'",
    "server:spec": "NODE_ENV=TEST rimraf .tmp && mocha-webpack --webpack-config webpack.server.config.spec.js --reporter spec --growl --colors --timeout 10000 --require source-map-support/register server/spec/helper.js \"server/**/*.spec.js\"",
    "server:spec:ci": "npm run server:spec -s",
    "server:coverage": "rimraf reports && cross-env NODE_ENV=TEST nyc --reporter=text --reporter=text-summary mocha-webpack --opts config/mocha/mocha-webpack-server.opts",
    "server:coverage:ci": "npm run server:coverage -s",
    "server:spec:watch": "npm run server:spec -- --watch",
    "server:tdd": "npm run server:spec:watch",
    "spec": "rimraf .tmp && node $NODE_DEBUG_OPTION ./node_modules/.bin/mocha-webpack --opts config/mocha/mocha-webpack.opts",
    "spec:ci": "npm run spec -s",
    "spec:browser": "rimraf .tmp && webpack-dev-server --config config/webpack.test.browser.js --inline --progress --port 8888",
    "spec:watch": "npm run spec -- --watch",
    "spec:server": "webpack-dev-server --config config/webpack.test.browser.js --inline --progress --port 8888",
    "spec:coverage": "rimraf reports && cross-env NODE_ENV=coverage nyc --reporter=text --reporter=text-summary mocha-webpack --opts config/mocha/mocha-webpack.opts",
    "spec:coverage:ci": "npm run spec:coverage -s",
    "spec:reports": "rimraf reports && cross-env NODE_ENV=coverage nyc mocha-webpack --opts config/mocha/mocha-webpack.opts --reporter=xunit --reporter-options output=reports/tests/tests-report.xml",
    "pact:server": "node contracts/dev-server/pact-server.js",
    "publish:local": "node contracts/broker/publish.js https://dataprivacy-portal-pact-broker.mybluemix.net",
    "tdd": "npm run spec:watch"
  },
  "dependencies": {
    "@angular/animations": "5.2.6",
    "@angular/cdk": "5.2.2",
    "@angular/common": "5.2.6",
    "@angular/compiler": "5.2.6",
    "@angular/core": "5.2.6",
